/* 地址信息 */
.addr-info {
  padding: 30rpx 70rpx 30rpx 90rpx;
  line-height: 1;
  background: url('data:image/png;base64,') 30rpx center no-repeat, url('data:image/png;base64,') 690rpx center no-repeat;
  background-size: 33rpx 38rpx;
  background-color: #fff;
  color: #353535;
  font-size: 32rpx;
  margin: 20rpx 0 20rpx;
}
/* 订单列表 - 子列表包裹层 */
.items-list {
  background-color: #fff;
  margin-bottom: 20rpx;
}
/* 订单列表 - 子列表标题 */
.items-list-tit {
  color: #999;
  line-height: 90rpx;
  font-size: 32rpx;
  padding-left: 80rpx;
  padding-right: 50rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,') no-repeat 35rpx center;
  background-size: 36rpx 36rpx;
}
/* 订单列表 - 子列表单个项目 */
.item {
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx 30rpx;
  overflow: hidden;
  border-top: 1rpx solid #ddd;
}
/* 订单列表 - 子列表单-项目商品图 */
.item .item-img {
  width: 180rpx;
  height: 180rpx;
  margin-right: 30rpx;
  display: block;
}
/* 订单列表 - 子列表单-项目信息 */
.item .item-info {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
}
/* 订单列表 - 子列表单-商品名称 */
.item .item-tit {
   font-size: 32rpx;
   color: #000;
   width: 460rpx;
   line-height: 50rpx;
 }
 /* 订单列表 - 子列表单-商品描述 */
.item .item-desc {
  color: #a9a9a9;
  font-size: 30rpx;
  width: 430rpx;
  line-height: 36rpx;
}
/* 订单列表 - 子列表单-商品属性 */
.item .item-attribute {
  overflow: hidden;
  width: 100%;
  line-height: 50rpx;
}
/* 订单列表 - 子列表单-商品规格 */
.item .item-spec {
  font-size: 26rpx;
  color: #a9a9a9;
  float: left;
}
/* 订单列表 - 子列表单-商品价格 */
.item .item-price {
  font-size: 30rpx;
  color: #e4383b;
  float: right;
  position: relative;
  padding-left: 20rpx;
}
.item .item-price::after {
  position: absolute;
  left: 0;
  bottom: 0;
  content: "¥";
  color: #e4383b;
  font-size: 20rpx;
}
/* 结算 */
.settlement {
  overflow: hidden;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.settlement .settlement-list {
  color: #999;
  font-size:32rpx;
  padding: 0 30rpx;
  line-height: 80rpx;
  overflow: hidden;
}
.settlement-list .list-key{float: left;}
.settlement-list .list-val {float: right;}
.settlement-total {
    border-top: 1px solid #ddd;
    line-height: 100rpx;
    font-size: 36rpx;
    text-align: right;
    padding: 0 30rpx;
}
.settlement-total .total-price {color: #e4383b;}

/* 结算按钮 */
.page-btn {
  margin: 40rpx 30rpx 60rpx;
  color: #fff;
}

/* 赠品样式 */
.gift-row {
  overflow: hidden;
  padding: 15rpx 30rpx;
  font-size: 30rpx;
  line-height: 1.5;
  color: #a9a9a9;
  border-top: 1rpx solid #ddd; 
}
.gift-name {
  float: left;
}
.gift-name text {
  padding: 5rpx 10rpx;
  border-radius: 6rpx;
  background-color: #e4383b;
  color: #fff;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.gift-qty {
  float: right;
}