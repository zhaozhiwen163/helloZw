/* pages/addrList/addrList.wxss */
.addr-list {
    overflow: hidden;
    line-height: 50rpx;
    padding: 20rpx 30rpx;
    background-color: #fff;
    padding-right: 0;
    margin-top: 30rpx;
    display: flex;
}
.addr-info {
  flex:1;
  width: 600rpx;
  font-size: 28rpx;
}
.consignee {font-size: 32rpx;display: block;}
.isDefault{
    font-size:28rpx;
    color:red;
}
.addr-edit {
  width: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 36rpx 36rpx;
}
/* 新增地址按钮 */
.creatAddr-btn {
    margin: 30rpx;
    color: #fff;
    font-size: 36rpx;
    background-color: #390;
    text-align: center;
    line-height: 90rpx;
    border-radius: 10rpx;
}
