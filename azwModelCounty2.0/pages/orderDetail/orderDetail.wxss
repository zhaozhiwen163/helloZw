/* pages/orderDetail/orderDetail.wxss */
@import "/utils/order.wxss";
/* 收件人信息 */
.addr-info {line-height: 1.5;}
.addr-info .address {font-size: 26rpx; padding-top: 20rpx;}
/* 结算合计 */
.settlement-total {text-align: left;}
.settlement-total text{float: right;}

/* 支付+取消 */
.settlement-btn {margin-bottom: 30rpx;}
/* 支付方式 */
.payment-method,.order-Info {
  overflow: hidden;
  margin-bottom: 20rpx;
  background-color: #fff;
  font-size: 36rpx;
  color: #353535;
  padding: 30rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
}
.payment-method .method-key,.order-Info .method-key {float: left;} 
.payment-method .method-val,.order-Info .method-val {float: right;} 
.order-Info {
  font-size: 30rpx;
  margin: 0;
  border-bottom: 1rpx solid #f2f2f2;
}
.addr-info {
padding:30rpx 80rpx;
line-height:1;
background:url('data:image/png;base64,') 30rpx center no-repeat;
background-size:33rpx 38rpx;
background-color:#fff;
color:#353535;
font-size:32rpx;
margin:20rpx 0 20rpx;

}
